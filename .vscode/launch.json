	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Debug Server (built)",
			"program": "${workspaceFolder}/server/dist/index.js",
			"preLaunchTask": "npm: build",
			"outFiles": [
				"${workspaceFolder}/server/dist/**/*.js"
			],
			"sourceMaps": true,
			"cwd": "${workspaceFolder}/server",
			"console": "integratedTerminal"
		}
	]
}